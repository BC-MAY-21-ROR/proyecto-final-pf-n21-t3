<%= link_to 'Back', profiles_path%>
<%= render 'about' %>
<% if current_user.id == @user.id %>
  <%= link_to 'Edit', edit_profile_path(current_user.id)%>
<% end %>
<h1>Skills <%= @user.name %> </h1>
<% @skills.each do |skill|%>
  <div>
    <p> <%= skill.name %> </p>
  </div>
<% end %>
<% if current_user.id == @user.id %>
  <%= link_to 'new', new_skill_path(current_user.id)%>
<% end %>
